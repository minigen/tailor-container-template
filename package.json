{
  "name": "tailor-react-spa",
  "version": "1.0.0",
  "description": "An exploration into using Tailor with React",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "node-tailor": "^3.9.2",
    "webpack": "^4.35.0"
  }
}
